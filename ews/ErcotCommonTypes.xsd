	<!--The type definitions in this XSD are potentially used across many interfaces-->
	<!---->
	<xs:simpleType name="SettlementPoint">
		<xs:annotation>
			<xs:documentation>Settlement point, identified by name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="SettlementPointType">
		<xs:annotation>
			<xs:documentation>Settlement point type, identified by name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="ElectricalBus">
		<xs:annotation>
			<xs:documentation>Electrical bus, identified by name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="URL">
		<xs:annotation>
			<xs:documentation>URL</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:anyURI"/>
	</xs:simpleType>
	<xs:simpleType name="QSE">
		<xs:annotation>
			<xs:documentation>QSE type, identified by name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="ASType">
		<xs:annotation>
			<xs:documentation>Ancillary services type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Reg-Up"/>
			<xs:enumeration value="Reg-Down"/>
			<xs:enumeration value="Non-Spin"/>
			<xs:enumeration value="REGUP-RRS-ONNS"/>
			<xs:enumeration value="Off-Non-Spin"/>
			<xs:enumeration value="On-Non-Spin"/>
			<xs:enumeration value="RRS"/>
			<xs:enumeration value="RRSGN"/>
			<xs:enumeration value="RRSLD"/>
			<xs:enumeration value="RRSNC"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AvailabilityType">
		<xs:annotation>
			<xs:documentation>Availability plan resource type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="RMR"/>
			<xs:enumeration value="SYNCCOND"/>
			<xs:enumeration value="BLACKSTART"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AVPStatus">
		<xs:annotation>
			<xs:documentation>Availability plan resource status</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="A"/>
			<xs:enumeration value="U"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BlockType">
		<xs:annotation>
			<xs:documentation>Block types</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="FIXED"/>
			<xs:enumeration value="VARIABLE"/>
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RUCType">
		<xs:annotation>
			<xs:documentation>RUC type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="DRUC"/>
			<xs:enumeration value="HRUC"/>
			<xs:enumeration value="DAM"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NetTradeType">
		<xs:annotation>
			<xs:documentation>Net Trade type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="P"/>
			<xs:enumeration value="S"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OperatingMode">
		<xs:annotation>
			<xs:documentation>Operating mode type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ONRUC"/>
			<xs:enumeration value="ON"/>
			<xs:enumeration value="ONREG"/>
			<xs:enumeration value="ONTEST"/>
			<xs:enumeration value="ONOS"/>
			<xs:enumeration value="ONOSREG"/>
			<xs:enumeration value="ONDSRREG"/>
			<xs:enumeration value="ONDSR"/>
			<xs:enumeration value="OFF"/>
			<xs:enumeration value="OFFNS"/>
			<xs:enumeration value="ONEMR"/>
			<xs:enumeration value="ONRR"/>
			<xs:enumeration value="OUT"/>
			<xs:enumeration value="EMR"/>
			<xs:enumeration value="ONRGL"/>
			<xs:enumeration value="ONRL"/>
			<xs:enumeration value="OUTL"/>
			<xs:enumeration value="ONOPTOUT"/>
			<xs:enumeration value="ONCLR"/>
			<xs:enumeration value="OFFQS"/>
		    <xs:enumeration value="EMRSWGR"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Resource">
		<xs:annotation>
			<xs:documentation>Resource, identified by name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="CombinedCycle">
		<xs:annotation>
			<xs:documentation>Combined cycle configuration</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="Equipment">
		<xs:annotation>
			<xs:documentation>Equipment, identified by name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="EquipmentType">
		<xs:annotation>
			<xs:documentation>Equipment Type, identified by name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="Hub">
		<xs:annotation>
			<xs:documentation>Hub, identified by name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="Zone">
		<xs:annotation>
			<xs:documentation>Zone, identified by name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="EnergyConsumer">
		<xs:annotation>
			<xs:documentation>EnergyConsumer</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="DayType">
		<xs:annotation>
			<xs:documentation>Day type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="TransactionStatusType">
		<xs:annotation>
			<xs:documentation>Transaction status type for bids, offers, trades and schedules</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="SUBMITTED"/>
			<xs:enumeration value="ACCEPTED"/>
			<xs:enumeration value="PENDING"/>
			<xs:enumeration value="REJECTED"/>
			<xs:enumeration value="ERRORS"/>
			<xs:enumeration value="UNCONFIRMED"/>
			<xs:enumeration value="CANCELED"/>
			<xs:enumeration value="ACKNOWLEDGED"/>
		</xs:restriction>
	</xs:simpleType>
	<!--Types for bidding-->
	<xs:complexType name="CapacitySchedule">
		<xs:annotation>
			<xs:documentation>Capacity Schedule: value1Unit is MW</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="TmSchedule"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SelfASCapacitySchedule">
		<xs:annotation>
			<xs:documentation>Self Arranged AS Capacity Schedule: value1Unit is MW</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SelfASTmSchedule"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PriceSchedule">
		<xs:annotation>
			<xs:documentation>Price Schedules: value1Unit is $/MW</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PriceTmPoint"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EnergySchedule">
		<xs:annotation>
			<xs:documentation>MW Schedules: value1Unit is MW</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="TmSchedule"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DCTieEnergySchedule">
		<xs:annotation>
			<xs:documentation>MW Schedules: value1Unit is MW</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="DCTieTmSchedule"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Error">
		<xs:annotation>
			<xs:documentation>Error structure</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="severity" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="ERROR"/>
						<xs:enumeration value="WARNING"/>
						<xs:enumeration value="INFORMATIVE"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="area" type="xs:string" minOccurs="0"/>
			<xs:element name="interval" type="xs:string" minOccurs="0"/>
			<xs:element name="text" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BidPriceCurve">
		<xs:annotation>
			<xs:documentation>Relationship between a price in $/hour (Y-axis) and a MW value (X-axis). </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="startTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Start time to identify when curve is used</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>End time to indicate when curve is no longer used</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="curveStyle" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Style of curve: FIXED, VARIABLE, CURVE</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="FIXED"/>
						<xs:enumeration value="VARIABLE"/>
						<xs:enumeration value="CURVE"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CurveData" maxOccurs="10">
				<xs:complexType>
					<xs:annotation>
						<xs:documentation>The point data values that define a curve</xs:documentation>
					</xs:annotation>
					<xs:sequence>
						<xs:element name="xvalue" type="xs:float">
							<xs:annotation>
								<xs:documentation>The data value of the X-axis variable,  depending on the X-axis units</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="y1value" type="xs:float">
							<xs:annotation>
								<xs:documentation>The data value of the  first Y-axis variable, depending on the Y-axis units</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="incExcFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Inclusive/Exclusive flag for three part offers to identify whether ancillary services are included or excluded</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="INC"/>
						<xs:enumeration value="EXC"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="reason" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Reason for adjustment</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="OUT"/>
						<xs:enumeration value="FUEL"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PriceCurve">
		<xs:annotation>
			<xs:documentation>Relationship between a price in $/hour (Y-axis) and a MW value (X-axis). </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="startTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Start time to identify when curve is used</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>End time to indicate when curve is no longer used</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="curveStyle" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Style of curve: FIXED, VARIABLE, CURVE</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="FIXED"/>
						<xs:enumeration value="VARIABLE"/>
						<xs:enumeration value="CURVE"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CurveData" maxOccurs="10">
				<xs:complexType>
					<xs:annotation>
						<xs:documentation>The point data values that define a curve</xs:documentation>
					</xs:annotation>
					<xs:sequence>
						<xs:element name="xvalue" type="MWSingleDecimal">
							<xs:annotation>
								<xs:documentation>The data value of the X-axis variable,  depending on the X-axis units</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="y1value" type="ErcotPrice">
							<xs:annotation>
								<xs:documentation>The data value of the  first Y-axis variable, depending on the Y-axis units</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="incExcFlag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Inclusive/Exclusive flag for three part offers to identify whether ancillary services are included or excluded</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="INC"/>
						<xs:enumeration value="EXC"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="reason" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Reason for adjustment</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="OUT"/>
						<xs:enumeration value="FUEL"/>
						<xs:enumeration value="DSCM"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="multiHourBlock" type="xs:boolean" default="false" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AASPriceCurve">
		<xs:annotation>
			<xs:documentation>AggregatedASOfferCurve Price Curve </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="startTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Start time to identify when curve is used</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>End time to indicate when curve is no longer used</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurveData" maxOccurs="unbounded">
				<xs:complexType>
					<xs:annotation>
						<xs:documentation>The point data values that define a curve</xs:documentation>
					</xs:annotation>
					<xs:sequence>
						<xs:element name="xvalue" type="xs:decimal">
							<xs:annotation>
								<xs:documentation>The data value of the X-axis variable,  depending on the X-axis units</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="y1value" type="xs:decimal">
							<xs:annotation>
								<xs:documentation>The data value of the  first Y-axis variable, depending on the Y-axis units</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ASPriceCurve">
		<xs:annotation>
			<xs:documentation>Defines curves for AS offers</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="startTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Start time to identify when curve is used</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="endTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>End time to indicate when curve is no longer used</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="OnLineReserves" maxOccurs="5">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="xvalue" type="MWSingleDecimal">
								<xs:annotation>
									<xs:documentation>MW quantity</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="REGUP" type="ErcotPrice" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Reg-Up price</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="RRS" type="ErcotPrice" minOccurs="0">
								<xs:annotation>
									<xs:documentation>RRS price</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ONNS" type="ErcotPrice" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Online Non-Spin price</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="block" type="BlockType">
								<xs:annotation>
									<xs:documentation>block type</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="RegDown" maxOccurs="5">
