	<!--Simple Type Definitions-->
	<xs:simpleType name="MWSingleDecimal_Orig">
		<xs:restriction base="xs:decimal">
			<xs:maxInclusive value="9999.9"/>
			<xs:minInclusive value="-9999.9"/>
			<xs:pattern value="[+\-]?(\d{1,4}|\d{1,4}\.\d{0,1}|\.\d{1})"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MWSingleDecimal" final="">
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="ErcotPrice">
		<xs:restriction base="xs:decimal">
			<xs:pattern value="[+\-]?(\d{1,6}|\d{1,6}\.\d{0,2}|\.\d{1,2})"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ErcotResTime">
		<xs:restriction base="xs:decimal">
			<xs:maxInclusive value="999999.9"/>
			<xs:minInclusive value="0"/>
			<xs:pattern value="[+\-]?(\d{1,6}|\d{1,6}\.\d{0,1}|\.\d{1})"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ErcotTime">
		<xs:restriction base="xs:decimal">
			<xs:maxInclusive value="999999.9"/>
			<xs:minInclusive value="-999999.9"/>
